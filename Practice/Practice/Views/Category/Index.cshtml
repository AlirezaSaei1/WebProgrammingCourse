@model IEnumerable<Category>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Index Category</h2>

<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>
            Category Name
        </th>
        <th>
            Category Order
        </th>
    </tr>
    </thead>
    
    <tbody>
        @foreach (var obj in Model!)
        {
            <tr>
                <td width="50%">
                    @obj.Name
                </td>
                <td width="30%">
                    @obj.DisplayOrder
                </td>
            </tr>
        }
    </tbody>
</table>